{
  "SampleApiClientConfig": {
    "BaseUrl": "https://kontofonmonitorapi-dev.azurewebsites.net/v1/"
  },
  "AzureApiClientConfig": {
    "BaseUrl": "https://kontofonmonitorapi-dev.azurewebsites.net/v1/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    },
    "NLog": {
      "LogLevel": {
        "Default": "Debug"
      }
    }
  },
  "NLog": {
    "targets": {
      "file": {
        "type": "File",
        "fileName": "C:/logs/apicaller.txt",
        "layout": "${longdate} ${level} ${message} ${exception}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "file"
      }
    ]
  }
}