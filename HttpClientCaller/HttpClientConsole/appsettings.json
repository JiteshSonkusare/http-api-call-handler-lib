{
  "SampleApiClientConfig": {
    "BaseUrl": "https://kontofonmonitorapi-dev.azurewebsites.net/v1/"
  },
  "AzureApiClientConfig": {
    "BaseUrl": "https://kontofonmonitorapi-dev.azurewebsites.net/v1/"
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "async": true,
      "logfile": {
        "type": "File",
        "fileName": "c:/logs/http-client-caller-${shortdate}.log"
      },
      "logconsole": {
        "type": "Console"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "logfile"
      },
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "logfile"
      },
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "logfile"
      },
      {
        "logger": "*",
        "minLevel": "Warn",
        "writeTo": "logfile"
      },
      {
        "logger": "*",
        "minLevel": "Error",
        "writeTo": "logfile"
      }
    ]
  }
}